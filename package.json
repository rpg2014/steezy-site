{
  "name": "steezy-site",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "analyze": "ANALYZE=true next build",
    "build-wasm": "wasm-pack build steezy-wasm ; jq '. + {type: \"module\", main: \"steezy-wasm.js\"} | del(.module)' steezy-wasm/pkg/package.json | sponge steezy-wasm/pkg/package.json",
    "link-wasm": "cd steezy-wasm/pkg ; npm link ; cd - ; npm link steezy-wasm",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-amplify/datastore": "^3.7.5",
    "@next/bundle-analyzer": "^12.0.10",
    "@types/amplify": "^1.1.25",
    "@types/react-bootstrap": "^0.32.29",
    "aws-amplify": "^4.3.13",
    "bootstrap": "^5.1.3",
    "file-loader": "^6.2.0",
    "next": "12.0.9",
    "next-pwa": "^5.4.4",
    "react": "17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.1.1",
    "sass": "^1.49.0"
  },
  "devDependencies": {
    "@types/node": "17.0.13",
    "@types/react": "17.0.38",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "typescript": "4.5.5"
  }
}
